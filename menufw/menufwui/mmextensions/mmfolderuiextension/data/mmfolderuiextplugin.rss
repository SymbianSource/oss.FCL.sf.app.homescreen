RESOURCE DIALOG r_menu_filename_query
	{
	flags = EGeneralQueryFlags;
	buttons = R_AVKON_SOFTKEYS_OK_CANCEL;
	items=
		{
		DLG_LINE
			{
			type = EAknCtQuery;
			id = EGeneralQuery;
			control= AVKON_DATA_QUERY
				{
				layout = EDataLayout;
				control = EDWIN
					{
					flags= EEikEdwinNoHorizScrolling | EEikEdwinResizable;
					maxlength = 256; // KMaxFileName
					};
				};
			}
		};
	}


				// Info for CMmFolderUIExtPlugin
				IMPLEMENTATION_INFO
					{
					implementation_uid = 0x2001CB79;
					version_no = 1;
					display_name = "Folder UI Extension Plugin";
					default_data = "Folder";
					opaque_data = "";
					}
				};
			}
		};
	}


USERINCLUDe     ../../../inc

LIBRARY euser.lib
LIBRARY estor.lib
LIBRARY eikcore.lib
LIBRARY cone.lib
LIBRARY ws32.lib
LIBRARY apgrfx.lib
LIBRARY fbscli.lib
LIBRARY avkon.lib

LIBRARY tsfswengine.lib
LIBRARY tsfswutils.lib

SOURCEPATH      ../data
START RESOURCE  hgteleport.rss
HEADER
TARGETPATH      APP_RESOURCE_DIR
LANGUAGE_IDS
END

SOURCEPATH      ../data
START RESOURCE  hgteleport_reg.rss
DEPENDS hgteleport.rsg
TARGETPATH      /private/10003a3f/apps
END

LIBRARY         oommonitor.lib
LIBRARY         hal.lib
LIBRARY         gfxtrans.lib
LIBRARY         akntransitionutils.lib
LIBRARY         bafl.lib
LIBRARY         akntouchgesturefw.lib
LIBRARY         aknphysics.lib
